$(function(){
  // initialize
  $("input[type=checkbox]:not(:checked)").parent("label").hide();

  // hook event
  $('input[type=search]').keyup(function(){
    var q = $(this).val();
    $("input[type=checkbox]").parent("label:not(:contains('" + q + "'))").hide();
    $("input[type=checkbox]").parent("label:contains('" + q + "')").show();
    $("input[type=checkbox]:checked").parent("label").show();
  });
});
