$(function(){
  // initialize
  $("input[type=checkbox]:not(:checked)").parent("li").hide();

  // hook event
  $('input[type=search]').keyup(function(){
    var q = $(this).val();
    $("input[type=checkbox]").parent("li").hide();
    $("input[type=checkbox]").parent("li:contains('" + q + "')").show();
    $("input[type=checkbox]:checked").parent("li").show();
  });
});

